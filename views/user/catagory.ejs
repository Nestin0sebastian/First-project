<script>
    let currentPage = 1;
    const itemsPerPage = 6; // Number of items to show per page

    // Function to display products for the current page
    function displayProducts() {
        // Logic to calculate the start and end index of products based on the current page
        const startIndex = (currentPage - 1) * itemsPerPage;
        const endIndex = startIndex + itemsPerPage;

        // Logic to show/hide products based on the current page
        const products = document.querySelectorAll('.products .product');
        products.forEach((product, index) => {
            if (index >= startIndex && index < endIndex) {
                product.style.display = 'block';
            } else {
                product.style.display = 'none';
            }
        });

        // Update the current page display
        document.querySelector('.current-page').innerText = currentPage;
    }

    // Function to go to the previous page
    function prevPage() {
        if (currentPage > 1) {
            currentPage--;
            displayProducts();
        }
    }

    // Function to go to the next page
    function nextPage() {
        const totalProducts = document.querySelectorAll('.products .product').length;
        const totalPages = Math.ceil(totalProducts / itemsPerPage);

        if (currentPage < totalPages) {
            currentPage++;
            displayProducts();
        }
    }

    // Initial display when the page loads
    displayProducts();
</script>

