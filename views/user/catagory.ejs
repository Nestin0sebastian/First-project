                                                // Update URL parameters or form data (depending on your backend)
                                                const queryParams = new URLSearchParams(window.location.search);
                                                queryParams.set('categories', selectedCategories.join(','));
                                        
                                                // Reload the page with updated parameters
                                                window.location.href = `${window.location.pathname}?${queryParams.toString()}`;
                                            }
                                        
                                            // On page load, check the checkboxes based on URL parameters
                                            window.onload = function () {
                                                const queryParams = new URLSearchParams(window.location.search);
                                                const selectedCategories = queryParams.getAll('categories');
                                        
                                                selectedCategories.forEach(category => {
                                                    const checkbox = document.getElementById(`cat-${category}`);
                                                    if (checkbox) {
