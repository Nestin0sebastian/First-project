    <script>
   const searchButton = document.getElementById('searchButton');
const productSearchInput = document.getElementById('productSearch');
const selectedCategories = document.querySelectorAll('input[name="SelectedCategories"]');

searchButton.addEventListener('click', function () {
    const searchQuery = productSearchInput.value.trim();
    const checkedCategories = Array.from(selectedCategories)
        .filter((checkbox) => checkbox.checked)
        .map((checkbox) => checkbox.value);

    const queryString = buildQueryString(searchQuery, checkedCategories);

    // Redirect to the new URL
    window.location.href = `/shop${queryString}`;
});

selectedCategories.forEach((checkbox) => {
    checkbox.addEventListener('change', function () {
        const searchQuery = productSearchInput.value.trim();
        const checkedCategories = Array.from(selectedCategories)
            .filter((checkbox) => checkbox.checked)
            .map((checkbox) => checkbox.value);

        const queryString = buildQueryString(searchQuery, checkedCategories);

        // Replace the current URL without triggering a page reload
        window.history.replaceState(null, null, `/shop${queryString}`);
    });
});

function buildQueryString(searchQuery, checkedCategories) {
    let queryString = '';

    if (searchQuery.length > 0) {
        queryString += `?search=${encodeURIComponent(searchQuery)}`;
    }

    if (checkedCategories.length > 0) {
        const categoryParam = `selectedCategories=${checkedCategories.join('&selectedCategories=')}`;
        queryString += queryString.length > 0 ? `&${categoryParam}` : `?${categoryParam}`;
    }

    return queryString;
}

</script>
