                });

                // Reload the page after successfully removing the coupon
                window.location.reload();
            }
        } catch (error) {
            console.error('Error:', error);

            // Display a Swal alert for errors
            await Swal.fire({
                icon: 'error',
                title: 'Error',
                text: 'An error occurred while removing the coupon.',
