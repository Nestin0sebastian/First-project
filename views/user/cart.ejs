<script>
    function updateQuantity(productId, change, event) {
        const quantityInput = document.getElementById(`quantityInput-${productId}`);
        const quantityDisplay = document.getElementById(`quantityDisplay-${productId}`);
        const currentQuantity = parseInt(quantityInput.value, 10);
        const minQuantity = parseInt(quantityInput.getAttribute('min'), 10);
        const maxQuantity = parseInt(quantityInput.getAttribute('max'), 10);
    
        const newQuantity = currentQuantity + change;
    
        if (newQuantity >= minQuantity && newQuantity <= maxQuantity) {
            // Prevent the default form submission
            event.preventDefault();
    
            // Make an AJAX request to update the quantity
            fetch('/quantitypost', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    productId: productId,
                    quantity: newQuantity
                })
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    // Update the displayed quantity
                    quantityInput.value = newQuantity;
                    quantityDisplay.innerText = newQuantity;
                    console.log('Quantity updated successfully:', data.cart);
                    
                    // Reload the page after updating quantity
                    location.reload();
                } else {
                    // Handle error messages or other responses from the server
                    console.error('Error updating quantity:', data.message);
                }
            })
            .catch(error => {
                console.error('Error updating quantity:', error);
            });
        }
    }
    </script>
    
    
<script>
    async function applyCoupon(id) {
        try {
            // alert(id);
            selectedcopon = id;
    
            // Fetch method to send data to the backend
            const response = await fetch('/admin/applycoupon', {
                method: 'POST', // You can use 'GET' or 'POST' based on your server setup
                headers: {
                    'Content-Type': 'application/json',
                },
                body: JSON.stringify({ couponId: selectedcopon }), // Convert the data to JSON
            });
    
            if (!response.ok) {
                throw new Error(`HTTP error! Status: ${response.status}`);
            }
    
            const data = await response.json();
            console.log(data);
    
            // Display SweetAlert for success
            if (data && data.message === 'Coupon applied successfully') {
                Swal.fire({
                    icon: 'success',
                    title: 'Coupon Applied Successfully',
                    text: `Discounted Total: ${data.discountedTotal}`,
                }).then(() => {
                    // Reload the page after the alert is closed
                    window.location.reload();
                });
            } else if (data && data.message === 'Coupon is not applicable to the current total price') {
                Swal.fire({
                    icon: 'error',
                    title: 'Coupon is not applicable to the current total price',
                });
            } else if (data && data.message === 'You have already applied a coupon') {
                Swal.fire({
                    icon: 'error',
                    title: 'You have already applied a coupon',
                });
            } else if (data && data.message === 'Coupon has already been applied') {
                Swal.fire({
                    icon: 'error',
                    title: 'Coupon has already been applied',
                });
            } else if (data && data.message === 'Coupon has expired') {
                Swal.fire({
                    icon: 'error',
                    title: 'Coupon has expired',
                });
            } else {
                // Display SweetAlert for errors
                Swal.fire({
                    icon: 'error',
                    title: 'Coupon Error',
                    text: data.message || 'An error occurred while applying the coupon.',
                });
            }
    
            // You can perform additional actions here, such as updating the UI
        } catch (error) {
            console.error('Error:', error);
    
            // Display SweetAlert for unexpected errors
            Swal.fire({
                icon: 'error',
                title: 'Unexpected Error',
                text: 'An unexpected error occurred. Please try again later.',
            });
        }
    }
</script>


