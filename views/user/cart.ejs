                    },
                    body: JSON.stringify({ couponId: couponId }),
                });

                if (!response.ok) {
                    throw new Error(`HTTP error! Status: ${response.status}`);
                }

                const data = await response.json();
                console.log(data);

                // Display a Swal alert for success
                await Swal.fire({
                    icon: 'success',
